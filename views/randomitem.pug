extends layout

block content
  h1 A random item
  - var image_names = ['Image 1.jpg', 'Image 2.jpeg', 'Image 3.jpeg', 'Image 4.jpeg', 'Image 5.jpeg']
  - for (let i = 0; i < 3; i++)
    - var randomIndex = Math.floor(Math.random() * 5)
    img(src=`/images/${image_names[randomIndex]}`, alt=image_names[randomIndex], height="300", width="300")